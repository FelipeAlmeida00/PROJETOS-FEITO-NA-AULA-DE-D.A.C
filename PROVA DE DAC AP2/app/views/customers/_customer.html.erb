<div id="<%= dom_id customer %>">
  <p>
    <strong>Nome:</strong>
    <%= customer.nome %>
  </p>

  <p>
    <strong>Sobrenome:</strong>
    <%= customer.sobrenome %>
  </p>

  <p>
    <strong>Data de nascimento:</strong>
    <%= customer.data_de_nascimento %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= customer.email %>
  </p>

  <p>
    <strong>Observacao:</strong>
    <%= customer.observacao %>
  </p>

</div>
